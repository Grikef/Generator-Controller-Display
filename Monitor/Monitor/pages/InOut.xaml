<Page x:Class="Monitor.pages.InOut"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Monitor.pages"
      mc:Ignorable="d" 
      d:DesignHeight="540" d:DesignWidth="1095"
      Title="InOut" Background="White">

    <Grid>
        <DockPanel LastChildFill="False">
            <Frame DockPanel.Dock="Left" x:Name="InFrame" Width="500"/>
            <Frame DockPanel.Dock="Right" x:Name="OutFrame" Width="500"/>
        </DockPanel>
    </Grid>
</Page>         